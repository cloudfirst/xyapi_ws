server
{
    listen 80;
    server_name web.sinyi.com.tw; # 这里填自定义域名
    location / {
        proxy_redirect off;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_pass http://172.17.0.2:5000; # 这里填容器地址，如果不知道填写公网IP也是可以的。
    }
}